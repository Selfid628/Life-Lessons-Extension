    <RecipeDef Name="WritingTextbooksBase" Abstract="True">
        <label>write textbook</label>
        <description>Write a textbook.</description>
        <jobString>Write a textbook.</jobString>
        <workSpeedStat>ResearchSpeed</workSpeedStat>
        <workSkill>Intellectual</workSkill>
        <effectWorking>Sculpt</effectWorking>
        <soundWorking>Recipe_Write</soundWorking>
        <workAmount>334000</workAmount>
        <unfinishedThingDef>VBE_UnfinishedBook</unfinishedThingDef>
        <recipeUsers>
            <li>VBE_WritersTable</li>
            <li>VBE_TypewritersTable</li>
        </recipeUsers>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Leathers</li>
                    </categories>
                </filter>
                <count>20</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Cloth</li>
                    </thingDefs>
                </filter>
                <count>75</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Cloth</li>
            </thingDefs>
            <categories>
                <li>Leathers</li>
            </categories>
        </fixedIngredientFilter>
    </RecipeDef>
